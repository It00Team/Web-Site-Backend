{
    "version": 2,
    "builds": [
        {
            "src": "./index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/getBlogs",
            "methods": ["GET"],
            "dest": "/"
        },
    {
            "src": "/(.*)",
            "dest": "/"
        }
    ]
}